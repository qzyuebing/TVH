
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option ddns_rundir '/var/run/ddns'
	option ddns_logdir '/var/log/ddns'

config service 'IPTV'
	option service_name 'oray.com'
	option use_ipv6 '0'
	option enabled '1'
	option password '534840997a'
	option interface 'wan'
	option use_syslog '2'
	option check_unit 'minutes'
	option retry_unit 'seconds'
	option check_interval '60'
	option force_interval '24'
	option force_unit 'hours'
	option retry_count '0'
	option retry_interval '60'
	option force_ipversion '1'
	option ip_source 'web'
	option bind_network 'wan'
	option ip_url 'http://whatismyip.akamai.com'
	option domain 'yuebing.iask.in'
	option username 'qzyuebing'
	option lookup_host 'yuebing.iask.in'

